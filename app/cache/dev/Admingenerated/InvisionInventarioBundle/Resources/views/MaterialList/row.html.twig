
{% block list_row %}
<tr class="list_trow">{% block list_row_content %}
                      <td class="td_VARCHAR td_nombre">
              {% block list_td_column_nombre %}{{ Material.nombre }}{% endblock list_td_column_nombre %}
          </td>
        {% endblock list_row_content %}
    {% block object_actions %}
                                <td class="object_actions">
                            <div class="btn-group">
                                    {% block object_action_edit %}
                            
                                        
    {% spaceless %}
    <a  class="btn btn-default btn-xs " href="{{ path("editar_material", { pk: Material.Id }) }}"
        data-original-title="{% trans from "Admingenerator" %}action.object.edit.label{% endtrans %}"
                rel="tooltip"
        ><i class="fa fa fa-pencil"></i></a>
    {% endspaceless %}
    
                                                {% endblock object_action_edit %}
                                    {% block object_action_delete %}
                            
                                        
    {% spaceless %}
    <a  class="btn btn-default btn-xs " href="{{ path("eliminar_material", { pk: Material.Id, action: 'delete' }) }}"
        data-original-title="{% trans from "Admingenerator" %}action.object.delete.label{% endtrans %}"
                rel="tooltip"
         data-confirm="{% trans from "Admingenerator" %}action.object.delete.confirm{% endtrans %}"  data-csrf-token="{{ path("eliminar_material", { pk: Material.Id, action: 'delete' })|csrf_token }}" ><i class="fa fa fa-eraser"></i></a>
    {% endspaceless %}
    
                                                {% endblock object_action_delete %}
                                {% block object_actions_customActions %}{% endblock object_actions_customActions %}
                </div>
                        </td>
                    {% endblock object_actions %}

</tr>
{% endblock list_row %}
