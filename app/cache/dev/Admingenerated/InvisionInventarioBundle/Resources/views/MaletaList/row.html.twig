
{% block list_row %}
<tr class="list_trow">{% block list_row_content %}
                      <td class="td_virtual td_vendedor">
              {% block list_td_column_vendedor %}{{ Maleta.usuario.username }}{% endblock list_td_column_vendedor %}
          </td>
                      <td class="td_INTEGER td_cantidad">
              {% block list_td_column_cantidad %}{{ Maleta.cantidad }}{% endblock list_td_column_cantidad %}
          </td>
        {% endblock list_row_content %}
    {% block object_actions %}
                                <td class="object_actions">
                            <div class="btn-group">
                                    {% block object_action_edit %}
                            
                                        
    {% spaceless %}
    <a  class="btn btn-default btn-xs " href="{{ path("editar_maleta", { pk: Maleta.Id }) }}"
        data-original-title="{% trans from "Admingenerator" %}action.object.edit.label{% endtrans %}"
                rel="tooltip"
        ><i class="fa fa fa-pencil"></i></a>
    {% endspaceless %}
    
                                                {% endblock object_action_edit %}
                                    {% block object_action_cargar %}
                            
                                        
    {% spaceless %}
    <a  class="btn btn-default btn-xs " href="{{ path("cargar_maleta", { 'pk': Maleta.Id, action: 'cargar' }) }}"
        data-original-title="{% trans from "Admin" %}Cargar{% endtrans %}"
                rel="tooltip"
        ><i class="fa fa fa-bars"></i></a>
    {% endspaceless %}
    
                                                {% endblock object_action_cargar %}
                                    {% block object_action_delete %}
                            
                                        
    {% spaceless %}
    <a  class="btn btn-default btn-xs " href="{{ path("Invision_InventarioBundle_Maleta_object", { pk: Maleta.Id, action: 'delete' }) }}"
        data-original-title="{% trans from "Admingenerator" %}action.object.delete.label{% endtrans %}"
                rel="tooltip"
         data-confirm="{% trans from "Admingenerator" %}action.object.delete.confirm{% endtrans %}"  data-csrf-token="{{ path("Invision_InventarioBundle_Maleta_object", { pk: Maleta.Id, action: 'delete' })|csrf_token }}" ><i class="fa fa-times"></i></a>
    {% endspaceless %}
    
                                                {% endblock object_action_delete %}
                                {% block object_actions_customActions %}{% endblock object_actions_customActions %}
                </div>
                        </td>
                    {% endblock object_actions %}

</tr>
{% endblock list_row %}
